<h1> </h1>


<h1>Listado Facturas</h1>

<div class ="media">
<div class ="media-body" style="overflow-x:auto;">

<%= form_tag search_invoices_path, method: :get do %>
  <%= text_field_tag :search,nil,placeholder: "Search", class:"form-control" , type:"buscar" %> 
  <%= submit_tag "buscar", class:"button"  %>
<% end %>


<%= render 'invoices/list' %>

<h2>Importar Archivo Facturas "*.csv"</h2>
<%= form_tag import_invoices_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Importar " %>
<% end %>


